<Window xmlns:Windows="clr-namespace:HIMTools.Windows" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HIMTools"
        xmlns:TabSystem="clr-namespace:HIMTools.Windows.TabSystem" x:Class="HIMTools.MapWindow"
        mc:Ignorable="d"
        Title="MapWindow" Height="450" Width="800">
    <Border>
        <DockPanel x:Name="ChromeBorder">
            <DockPanel x:Name="TitleBar" DockPanel.Dock="Top">
                <Border x:Name="MainContainer">
                    <Grid>
                        <Windows:MainPanel QuadTabOwner="{Binding QuadOwner}"/>
                        <Grid x:Name="SmakeScreen" Visibility="Collapsed"/>
                    </Grid>
                </Border>
            </DockPanel>
        </DockPanel>
    </Border>
</Window>
